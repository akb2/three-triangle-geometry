{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "declaration": true,
    "strict": true,
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "types": [
      "vite/client",
      "@storybook/preact"
    ]
  },
  "include": [
    "src"
  ]
}